@using Tne.Web.Dal
@model IEnumerable<CurrentTransformator>



<table class="table table-striped table-responsive">
    <thead>
        <tr>
            <td>Id</td>
            <td>Дата проверки</td>
            <td>Наименование</td>
            <td>Тип трансформатора</td>
            <td>Ктн</td>
        </tr>
    </thead>

    <tbody>

        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.CheckDate.ToShortDateString()</td>
            <td>@item.PointOfMeasure.Name</td>
            <td>@item.CurrentTransformatorType</td>
            <td>@item.Ktt</td>
        </tr>
        }

    </tbody>
</table>


